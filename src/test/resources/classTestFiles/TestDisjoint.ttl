@prefix ninepts:    	<http://purl.org/ninepts/test#> .
@prefix owl:    		<http://www.w3.org/2002/07/owl#> .
@prefix rdf:        	<http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

# Ontology defined
# Two classes, disjoint with each other

<http://purl.org/ninepts/test>
	rdf:type					owl:Ontology ;
	owl:versionInfo				"1.0" .

<http://purl.org/ninepts/test#class1>
	rdf:type				owl:Class ;
	owl:disjointWith		ninepts:class2 .

<http://purl.org/ninepts/test#class2>
	rdf:type				owl:Class .
